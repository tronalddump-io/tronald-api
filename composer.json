{
  "name": "tronald",
  "description": "tronalddump.io is a free JSON API the dumbest things donald trump has ever said.",
  "homepage": "https://api.chucknorris.io",
  "keywords": [
    "tonald",
    "tronald dump"
  ],
  "license": "GPL-3.0+",
  "type": "project",
  "require": {
    "php": "~7.2",
    "ext-imagick": "*",
    "ext-pgsql": "*",
    "auth0/auth0-php": "~4.0",
    "bramdevries/oauth2-slack": "~0.1.0",
    "bramus/monolog-colored-line-formatter": "~2.0",
    "doctrine/dbal": "~2.5",
    "gitory/pimple-cli": "~1.0",
    "jms/serializer": "~1.4",
    "monolog/monolog": "~1",
    "nocarrier/hal": "~0.9.12",
    "ramsey/uuid": "~3.5",
    "silex/silex": "~2.0",
    "symfony/browser-kit": "~3.2",
    "symfony/config": "~3.1",
    "symfony/console": "~3.1",
    "symfony/form": "~3.1",
    "symfony/translation": "~3.1",
    "symfony/twig-bridge": "~3.1",
    "symfony/security-csrf": "~3.1",
    "symfony/validator": "~3.1",
    "symfony/yaml": "~3.1",
    "twig/twig": "~1.27",
    "vlucas/phpdotenv": "~2.4",
    "league/json-guard": "~0.5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "squizlabs/php_codesniffer": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Tronald\\App\\Api\\": "module/api/src",
      "Tronald\\Test\\App\\Api\\": "module/api/test",
      "Tronald\\App\\Cli\\": "module/cli/src",
      "Tronald\\App\\Web\\": "module/web/src",
      "Tronald\\Lib\\": "module/lib/src"
    }
  },
  "prefer-stable": true,
  "scripts": {
    "lint": "./vendor/bin/phpcs --extensions=php --standard=PSR2 ./module/",
    "test": "./vendor/bin/phpunit --configuration phpunit.xml --coverage-text"
  }
}
